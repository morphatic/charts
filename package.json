{
  "name": "@morphatic/charts",
  "description": "A charting library for Angular 5+ that uses d3 and d3kit.",
  "version": "0.1.3",
  "repository": {
    "type": "git",
    "url": "https://github.com/morphatic/charts.git"
  },
  "homepage": "https://github.com/morphatic/charts",
  "bugs": "https://github.com/morphatic/charts/issues",
  "author": "Morgan Benton <morgan.benton@gmail.com> (https://morphatic.com)",
  "license": "MIT",
  "peerDependencies": {
    "d3": "^4.12.0",
    "d3kit": "^3.2.0"
  },
  "keywords": [
    "chart",
    "barchart",
    "d3",
    "d3kit",
    "angular",
    "components"
  ],
  "main": "bundles/morphatic-charts.umd.js",
  "module": "esm5/morphatic-charts.js",
  "es2015": "esm2015/morphatic-charts.js",
  "typings": "morphatic-charts.d.ts",
  "metadata": "morphatic-charts.metadata.json",
  "scripts": {
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@angular/common": "^5.2.11",
    "@angular/compiler": "^5.2.11",
    "@angular/core": "^11.0.5",
    "@angular/platform-browser": "^5.2.11",
    "@angular/platform-browser-dynamic": "^5.2.11",
    "@types/d3": "^4.13.2",
    "@types/d3kit": "^3.2.2",
    "@types/jest": "^21.1.10",
    "codelyzer": "^4.5.0",
    "core-js": "^3.1.3",
    "d3": "^4.13.0",
    "d3kit": "^3.2.0",
    "jest": "^24.8.0",
    "jest-preset-angular": "^7.1.1",
    "rxjs": "^5.5.12",
    "tslint": "^5.17.0",
    "typescript": "^2.9.2",
    "zone.js": "^0.8.29"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "html",
      "js"
    ],
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "collectCoverageFrom": [
      "src/**/*"
    ],
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "coverageDirectory": "./coverage"
  }
}
